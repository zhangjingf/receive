.title {
    padding: 28rpx 0 41rpx 0;
    font-size:28rpx;
    text-align: center;
    font-family:PingFang-SC-Medium;
    font-weight:500;
    color:rgba(74,74,74,1);
    background:rgba(255,255,255,1);
}
.money {
    font-size:50rpx;
    text-align: center;
    font-family:PingFang-SC-Bold;
    font-weight:bold;
    color:rgba(0,0,0,1);
    background:rgba(255,255,255,1);
}
.fees, .order {
  padding: 0 30rpx;
  font-size:28rpx;
  line-height: 78rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(0,0,0,1);
  background:rgba(255,255,255,1);
}
.fees>view, .order>view{
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid rgba(220,220,220,1);
}
.fees>view:last-child {
  border-bottom: none;
}
.order {
  margin-top: 20rpx;
}
.color {
  color: #999999;
}
